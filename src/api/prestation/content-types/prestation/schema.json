{
  "kind": "collectionType",
  "collectionName": "prestations",
  "info": {
    "singularName": "prestation",
    "pluralName": "prestations",
    "displayName": "Prestation",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titre": {
      "type": "string",
      "required": true,
      "default": "Prestation HPI"
    },
    "description": {
      "type": "text"
    },
    "illustration": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "phrase_accroche": {
      "type": "string"
    },
    "texte_accroche": {
      "type": "text"
    },
    "titre_cta": {
      "type": "string"
    },
    "texte_cta": {
      "type": "text"
    },
    "Metadatas": {
      "displayName": "SeoMetadatas",
      "type": "component",
      "repeatable": false,
      "component": "seo.seo-metadatas"
    },
    "couleur_page": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    }
  }
}
