{
  "kind": "collectionType",
  "collectionName": "prestations",
  "info": {
    "singularName": "prestation",
    "pluralName": "prestations",
    "displayName": "Prestation",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "titre": {
      "type": "string",
      "required": true,
      "default": "Prestation HPI",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "illustration": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "phrase_accroche": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "texte_accroche": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "titre_cta": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "texte_cta": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Metadatas": {
      "displayName": "SeoMetadatas",
      "type": "component",
      "repeatable": false,
      "component": "seo.seo-metadatas",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "couleur_page": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Image_et_texte": {
      "displayName": "ImgDisplay",
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "basic.img-display"
    },
    "Image_et_texte_du_bas": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "basic.img-display"
    },
    "Cta_du_bas": {
      "displayName": "bloc_cta",
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "basic.bloc-cta"
    },
    "bouton_du_haut": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "basic.bouton"
    },
    "zone_milieu": {
      "displayName": "wide_bloc_cta",
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "basic.wide-bloc-cta"
    }
